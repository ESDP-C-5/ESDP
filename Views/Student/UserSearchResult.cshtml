@model List<Student>

<div id="searchresults">
    @if (Model.Count > 0)
    {
        foreach (Student student in Model)
        {
            <span><a asp-action="Details" asp-controller="Student" asp-route-userName="@student.LastName">@student.LastName</a></span>
            <span>@student.Name</span>
        }
    }
</div>